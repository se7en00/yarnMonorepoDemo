{
      "restartable": "rs",
      "ignore": [".git", ".vscode", ".idea", "node_modules/", "test/", "*.test.ts", "build/"],
      "verbose": true,
      "execMap": {
            "ts": "node -r ts-node/register"
      },
      "env": {
            "NODE_ENV": "development"
      },
      "watch": ["./src/**/*.*"],
      "ext": "js,json,ts"
}
